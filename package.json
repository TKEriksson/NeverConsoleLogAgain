{
  "name": "neverconsolelogagain",
  "displayName": "NeverConsoleLogAgain",
  "description": "Press Ctrl + Alt + C (Windows/Linux) or Cmd + Option + C (macOS) to print out console.log();",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.100.0"
  },
  "publisher": "tobiaseriksson",
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:neverconsolelogagain.insertConsoleLog"
  ],
  "main": "./extension.js",
  "icon": "icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/TKEriksson/NeverConsoleLogAgain.git"
  },
  "bugs": {
    "url": "https://github.com/TKEriksson/NeverConsoleLogAgain/issues"
  },
  "homepage": "https://github.com/TKEriksson/NeverConsoleLogAgain",
  "contributes": {
    "commands": [
      {
        "command": "neverconsolelogagain.insertConsoleLog",
        "title": "Insert Console Log"
      }
    ],
    "keybindings": [
      {
        "command": "neverconsolelogagain.insertConsoleLog",
        "key": "ctrl+alt+c",
        "mac": "cmd+option+c",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.100.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "eslint": "^9.25.1",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.5.2"
  }
}
